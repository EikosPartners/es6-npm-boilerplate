{
  "name": "es6-npm-boilerplate",
  "version": "1.0.0",
  "description": "An example of boilerplate code for an ES6 package",
  "main": "dist/helloWorld.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "doc": "jsdoc -d ./doc/ src/",
    "test": "karma start test/karma_config/karma.config.js",
    "test-coverage": "rimraf test/coverage && karma start test/karma_config/karma.config.coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EikosPartners/es6-npm-boilerplate.git"
  },
  "author": "Zachary Donato <zachary.donato@eikospartners.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EikosPartners/es6-npm-boilerplate/issues"
  },
  "homepage": "https://github.com/EikosPartners/es6-npm-boilerplate",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-debug": "^0.1.2",
    "karma-mocha-reporter": "^2.0.4",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1",
    "karma-sourcemap-loader": "^0.3.7"
  },
  "dependencies": {
    "karma-webpack": "^1.7.0"
  }
}
