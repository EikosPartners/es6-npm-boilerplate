  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EikosPartners/es6-npm-boilerplate/issues"
  },
  "homepage": "https://github.com/EikosPartners/es6-npm-boilerplate",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "copyfiles": "^0.2.1"
  }
}
